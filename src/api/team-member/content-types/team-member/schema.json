{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "team member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "memberName": {
      "type": "string",
      "required": true
    },
    "designation": {
      "type": "string",
      "required": true
    },
    "memberProfilePic": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "memberBio": {
      "type": "text",
      "minLength": 20,
      "maxLength": 100
    },
    "socialLinks": {
      "type": "component",
      "component": "common.social-links",
      "repeatable": true
    }
  }
}
